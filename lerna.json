{
  "npmClient": "yarn",
  "version": "0.6.1",
  "packages": [
    "packages/*"
  ],
  "command": {
    "bootstrap": {
      "npmClientArgs": [
        "--no-lockfile",
        "--no-package-lock"
      ]
    },
    "publish": {
      "allowBranch": "master",
      "verifyAccess": false,
      "verifyRegistry": false,
      "registry": "https://registry.npmjs.org"
    }
  },
  "lerna": "2.11.0"
}
