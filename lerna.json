{
  "npmClient": "yarn",
  "version": "0.6.1",
  "packages": [
    "packages/*"
  ],
  "command": {
    "bootstrap": {
      "npmClientArgs": [
        "--no-lockfile",
        "--no-package-lock"
      ]
    },
    "publish": {
      "allowBranch": "master",
      "verifyAccess": true,
      "verifyRegistry": true,
      "registry": "http://registry.npmjs.org",
      "publishConfig": {
        "access": "public"
      }
    }
  },
  "lerna": "2.11.0"
}
