{
  "name": "{{name}}",
  "version": "{{version}}",
  "description": "{{description}}",
  "keywords": [
    "{{keywords}}"
  ],
  "main": "dist/index.js",
  "module": "dist/es/index.js",
  "files": [
    "dist",
    "README.md",
    "package.json"
  ],
  "author": "{{author}}<{{email}}>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "{{repository}}"
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-proposal-object-rest-spread": "^7.9.0",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.9.0",
    "@babel/preset-env": "^7.9.0",
    "@rollup/plugin-alias": "^3.0.1",
    "@rollup/plugin-commonjs": "^11.0.2",
    "@rollup/plugin-node-resolve": "^7.1.1",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.1.0",
    "console-log-h5": "^1.0.6",
    "cross-env": "^7.0.2",
    "eslint": "^6.8.0",
    "eslint-loader": "^3.0.3",
    "lodash": "^4.17.15",
    "ip": "^1.1.5",
    "npm-run-all": "^4.1.5",
    "rollup": "^2.1.0",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-replace": "^2.2.0",
    "webpack": "^4.42.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3"
  },
  "scripts": {
    "build": "npm-run-all -p \"build:**\"",
    "build:esm": "rollup -c --silent",
    "build:umd": "cross-env NODE_ENV=production webpack --mode=production",
    "clean": "rm -rf dist",
    "dev": "webpack-dev-server --watch",
    "dev:open": "webpack-dev-server --watch --open",
    "postversion": "git push --follow-tags",
    "push": "sh ./publish.sh -pm",
    "pub": "npm run build:esm && sh ./publish.sh -npm",
  }
}